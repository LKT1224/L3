<scroll-view class="container">
<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
 <block wx:for="{{imgUrls}}">
   <swiper-item>
     <image src="{{item}}"  style="height: 100%; width: 100%;"   class='img' />
  </swiper-item>
 </block>
</swiper>
</scroll-view>
